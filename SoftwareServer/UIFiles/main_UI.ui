}

QComboBox:hover {
    border: 1px solid #5e81ac;
}

QComboBox::drop-down {
    border: none;
}

QComboBox::down-arrow {
    image: url(:/icons/icons/icons8-drop-down-80.png);

    width: 12px;
    height: 12px;
}

QLineEdit {

    border: 1px solid #4c566a; /* Subtle border */
    border-radius: 5px; /* Rounded corners */
    padding: 5px 10px; /* Padding inside the line edit */
    color: #d8dee9; /* Text color */
    font-size: 14px; /* Font size */
    font-weight: 400; /* Font weight */
}

QLineEdit:focus {
    border: 1px solid #81a1c1; /* Border color on focus */
    background-color: #3b4252; /* Slightly lighter background color on focus */
    selection-background-color: #81a1c1; /* Selection background color */
    selection-color: #2e3440; /* Selection text color */
}

QLineEdit::placeholder {
    color: #4c566a; /* Placeholder text color */
    font-style: italic; /* Italicize placeholder text */
}


QToolButton {
    background-color: #4c566a; /* Dark background color */
    border: 1px solid #3b4252; /* Border color */
    border-radius: 5px; /* Rounded corners */
    padding: 5px 10px; /* Padding around the text/icon */
    color: #eceff4; /* Text color */
    font-size: 14px; /* Font size */
    font-weight: bold; /* Font weight */
    icon-size: 24px; /* Icon size */
}

QToolButton:hover {
    background-color: #5e81ac; /* Lighter background color on hover */
    border: 1px solid #4c566a; /* Border color on hover */
}

QToolButton:pressed {
    background-color: #434c5e; /* Darker background color on press */
    border: 1px solid #3b4252; /* Border color on press */
}

QToolButton::menu-indicator {
    image: url(:/icons/icons8-menu-24.png); /* Custom icon for the menu indicator */
    width: 16px; /* Width of the menu indicator */
    height: 16px; /* Height of the menu indicator */
}

QToolButton::separator {
    width: 2px; /* Width of the separator */
    background-color: #3b4252; /* Color of the separator */
}

QToolButton::toolbutton {
    padding: 5px; /* Padding for the toolbutton */
}


QCheckBox {
    spacing: 5px; /* Space between the checkbox and the text */
    font-size: 14px; /* Font size */
    font-weight: 400; /* Font weight */
}

QCheckBox::indicator {
    width: 20px; /* Width of the checkbox */
    height: 20px; /* Height of the checkbox */
    border: 1px solid #4c566a; /* Border color for the checkbox */
    border-radius: 3px; /* Rounded corners */

}

QCheckBox::indicator:checked {
background-color: #2e3440; /* Default background color */
    border: 1px solid #81a1c1; /* Border color when checked */
    image: url(:/icons/icons/icons8-check-mark-48.png); /* Checkmark icon path */
}

QCheckBox::indicator:hover {
    border: 1px solid #5e81ac; /* Border color on hover */
}

QCheckBox::indicator:pressed {
    background-color: #434c5e; /* Background color when pressed */
}

QCheckBox::indicator:checked:pressed {
    background-color: #3b4252; /* Background color when checked and pressed */
}

QCheckBox::indicator:unchecked:pressed {
    background-color: #3b4252; /* Background color when unchecked and pressed */
}

QCheckBox::indicator:checked:disabled {
    background-color: #4c566a; /* Background color when disabled and checked */
    border: 1px solid #4c566a; /* Border color when disabled and checked */
}

QCheckBox::indicator:unchecked:disabled {
    background-color: #2e3440; /* Background color when disabled and unchecked */
    border: 1px solid #2e3440; /* Border color when disabled and unchecked */
